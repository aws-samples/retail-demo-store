{
  "name": "retail-demo-store",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "layer0:start": "concurrently \"npx webpack --watch --config layer0/webpack.layer0.config.js\" \"npx layer0 run\"",
    "layer0:start:cache": "concurrently \"npx webpack --watch --config layer0/webpack.layer0.config.js\" \"npx layer0 run --cache\"",
    "layer0:start:prod": "npx layer0 run --production",
    "layer0:build": "npx webpack --progress --config layer0/webpack.layer0.config.js && npx layer0 build",
    "layer0:deploy": "npx layer0 deploy --skip-build",
    "layer0:update": "npm i -D @layer0/cli@latest && npm i @layer0/core@latest @layer0/prefetch@latest @layer0/devtools@latest"
  },
  "dependencies": {
    "@layer0/core": "^3.5.0",
    "@layer0/devtools": "^3.5.0",
    "@layer0/prefetch": "^3.5.0",
    "@optimizely/optimizely-sdk": "4.0.0",
    "amplitude-js": "^5.8.0",
    "aws-amplify": "^3.0.23",
    "aws-amplify-vue": "^2.1.2",
    "axios": "^0.21.1",
    "dotenv": "^8.2.0",
    "moment": "^2.24.0",
    "sweetalert": "^2.1.2",
    "uuid": "^7.0.3",
    "vue": "^2.6.11",
    "vue-agile": "^1.1.3",
    "vue-router": "^3.4.2",
    "vue-the-mask": "^0.11.1",
    "vuex": "^3.5.1",
    "vuex-persistedstate": "^3.1.0"
  },
  "devDependencies": {
    "@layer0/cli": "^3.5.0",
    "@vue/cli-plugin-babel": "^4.5.2",
    "@vue/cli-plugin-eslint": "^4.5.2",
    "@vue/cli-service": "^4.5.2",
    "babel-eslint": "^10.1.0",
    "concurrently": "^5.3.0",
    "eslint": "^6.8.0",
    "eslint-loader": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "is-buffer": "^2.0.4",
    "lodash": "^4.17.19",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^4.2.0"
  },
  "resolutions": {
    "acorn": "^6.4.1",
    "elliptic": "^6.5.3",
    "kind-of": "^6.0.3",
    "minimist": "^1.2.5",
    "selfsigned": "^1.10.8"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
